<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
	stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="pers.cmeu.controller.HistoryConfigController">
	<center>

	</center>
	<center>
		<AnchorPane prefHeight="200.0" prefWidth="200.0"
			styleClass="pane-padding" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="btnSaveClassConfig" layoutX="445.0" layoutY="521.0"
					mnemonicParsing="false" onAction="#saveClassConfig" styleClass="btn-success"
					text="保存">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<TableView fx:id="tblConfigInfo" prefHeight="469.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
				<CheckBox fx:id="chkGetAndSet" layoutX="14.0" layoutY="499.0"
					mnemonicParsing="false" selected="true" text="创建get与set方法">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
				<CheckBox fx:id="chkConstruct" layoutX="14.0" layoutY="527.0"
					mnemonicParsing="false" selected="true" text="创建无参构造方法">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
				<CheckBox fx:id="chkConstructAll" layoutX="14.0" layoutY="556.0"
					mnemonicParsing="false" selected="true" text="创建带参构造方法">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
				<Text layoutX="15.0" layoutY="488.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="实体类生成相关设置" />
				<Label layoutX="101.0" layoutY="579.0"
					text="温馨提示:系统启动时会默认加载default配置文件,如果是常用的配置可以将名字存储为default"
					AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" />
				<CheckBox fx:id="chkCreateJDBCtype" layoutX="170.0"
					layoutY="556.0" mnemonicParsing="false" text="Mapper文件指定JDBC数据类型">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
				<CheckBox fx:id="chkSerializable" layoutX="170.0" layoutY="527.0"
					mnemonicParsing="false" text="对象序列化:implements Serializable">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
				<CheckBox fx:id="chkUnlineCamel" layoutX="170.0" layoutY="499.0"
					mnemonicParsing="false" selected="true" text="去除下划线并以驼峰命名">
					<font>
						<Font size="14.0" />
					</font>
				</CheckBox>
			</children>
		</AnchorPane>
	</center>
</BorderPane>
