<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.syc.sh.domain">
	<class name="CityWeather" table="cityweather">
		<id name="id" column="id">
			<generator class="increment" />
		</id>

		<property name="country" />
		<property name="city" />
		<property name="recordDate" />
		<property name="weather" />
		<property name="wind" />
		<property name="temperature" />
		<property name="threeClock" />
		<property name="sixClock" />
		<property name="nineClock" />
		<property name="twelveClock" />
	</class>
</hibernate-mapping>    